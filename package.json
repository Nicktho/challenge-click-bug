{
  "name": "challenge-click-bug",
  "version": "1.0.0",
  "description": "Flood Element test script",
  "private": true,
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 100,
    "useTabs": true,
    "tabWidth": 2,
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "arrowParens": "avoid"
  },
  "dependencies": {
    "@flood/element": "^1.3.9",
    "@flood/element-cli": "^1.3.9",
    "assert": "*",
    "faker": "*",
    "prettier": "*"
  },
  "devDependencies": {
    "@types/faker": "*",
    "@types/assert": "*"
  },
  "element": {
    "testMatch": [
      "**/*.perf.[tj]s"
    ]
  },
  "scripts": {
    "run-bug": "element run challenge-click-bug.perf.ts --no-headless",
    "run-nobug": "element run challenge-click-bug.perf.ts"
  }
}